    "license": "MIT",
    "authors": [
        {
            "name": "Jose Gabriel Gonzalez",
            "email": "jgabrielsinner10@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-0": {
            "Crypt": "Phpseclib",
            "File": "Phpseclib",
            "Math": "Phpseclib",
            "Net": "Phpseclib"
        }
    }
